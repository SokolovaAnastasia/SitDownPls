- var nameclass = ''
- var list = ''
mixin product(nameclass, list)
    div(class="" + `${nameclass}__container`).flex
        span(class="" + `${nameclass}__grade`).star= list.span
        a(class="" + `${nameclass}__link link` href="card.html")
            img.imgreset.lazy(class="" + `${nameclass}__img` data-src="../imgs/" + list.img, alt="товары с высоким рейтингом")
        div(class="" + `${nameclass}__contain`)
            h3.hreset(class="" + `${nameclass}__subtitle`).secondarytitle= list.title
            p.preset.secondarytitle=list.subtitle
            p.preset(class="" + `${nameclass}__price`)= list.price
            button(class="" + `${nameclass}__btn`).btn.halfbtn Купить
mixin swiperCard
    .swiper-wrapper
        - var swipimg = [{number:'2'}, {number:'1'}, {number:'3'}, {number:'4'}, {number:'5'}]
            each list in swipimg
                .swiper-slide
                    img.imgreset.swiper-slide-img.lazy(data-src=`../imgs/cardLeftImgs${list.number}.svg` alt="изображение товара")
- var navheaderList = []
mixin breadCrumbs(navheaderList)
    .bottomheaderbasketnav
        ul.navheaderlist.listreset.flex
            - var navheaderList = navheaderList
                each listnavheader in navheaderList
                    li.navheaderlistitem
                        a.navheaderlistlink.link(href="#")= listnavheader.title
- var categoryList = []
- var ids = ''
mixin checkbox(categoryList, ids)
    ul.catalogleft__categorylist.listreset(id= ids)
        - var categoryList = categoryList
            each list in categoryList
                li.catalogleft__categoryitem.flex
                    input.catalogleft__inpcheck(id= list.id type='checkbox' name= list.name value= list.name)
                    label.catalogleft__spaninp(for= list.id)= list.name
